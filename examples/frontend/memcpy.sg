extern fun unsafe_memcpy(dst: &mut Cell, src: &Cell, size: Int);

fun memcpy<T>(dst: &mut T, src: &T, count: Int) {
    unsafe_memcpy(dst as &mut Cell, src as &Cell, count * sizeof<T>());
}

let src = [1, 2, 3, 4, 5];
let mut dst = [0, 0, 0, 0, 0];

print("src: ", src, "\ndst: ", dst, "\n");
memcpy<[Int * sizeof(src)]>(&mut dst, &src, 1);
print("src: ", src, "\ndst: ", dst, "\n");